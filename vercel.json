{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.allspade.com" }],
      "destination": "https://allspade.com/:path*",
      "permanent": true
    }
  ]
}
