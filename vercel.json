{
  "rewrites": [
    {
      "source": "/(sitemap\\.xml|robots\\.txt)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "https://www.allspade.com/:path*",
      "destination": "https://allspade.com/:path*",
      "permanent": true
    }
  ]
}
